{
  "name": "sefuri-salesfront",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --inline --port 8080",
    "clean": "rimraf ../public/dist .awcache",
    "build": "rimraf ../public/dist .awcache && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build_dev": "rimraf ../public/dist .awcache && webpack --config config/webpack.dev.local.js --progress --profile --bail",
    "webpack_dev_local": "webpack --config config/webpack.dev.local.js --progress --profile --bail",
    "watch": "rimraf ../public/dist .awcache && webpack --config config/webpack.dev.local.js --progress --profile --bail --watch",
    "postinstall": "typings install",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "angular2-datatable": "^0.5.2",
    "angular2-text-mask": "^8.0.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.5.1",
    "jquery-modal": "^0.8.0",
    "locale-index-of": "^1.0.2",
    "lodash": "^4.17.10",
    "moment": "^2.15.1",
    "ngx-tooltip": "0.0.9",
    "react-number-format": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "ts-md5": "^1.2.0",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "angular-router-loader": "^0.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "img-loader": "^1.3.1",
    "jasmine-core": "^2.4.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "typescript": "2.0.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
